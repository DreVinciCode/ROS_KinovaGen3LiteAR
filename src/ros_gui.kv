	MDLabel:
		id: Hand_Position
		text: "Horizontal Position:"
		font_size: '22sp'
	    pos_hint: {"center_x": 0.55, "center_y": 0.6}
		size_hint: 1,1

	MDRaisedButton:
	    text: "Left"
	    md_bg_color: 0, 0, 0.8, 0.5
	    pos_hint: {"center_x": 0.35, "center_y": 0.6}
	    on_press: app.horizontal_control_positive(*args)

	MDRaisedButton:
	    text: "Right"
	    md_bg_color: 0, 0, 0.8, 0.5
	    pos_hint: {"center_x": 0.45, "center_y": 0.6}
	    on_press: app.horizontal_control_negative(*args)

	MDLabel:
		id: Horizontal_Position
		text: "Value"
	    pos_hint: {"center_x": 1.4, "center_y": 0.6}
		size_hint: 1,1

	MDLabel:
		id: Spacer
		text: "Set Value: "
	    pos_hint: {"center_x": 1.2, "center_y": 0.6}
		size_hint: 1,1








	MDLabel:
		id: Hand_Position
		text: "Vertical Position:"
		font_size: '22sp'
	    pos_hint: {"center_x": 0.55, "center_y": 0.5}
		size_hint: 1,1

	MDRaisedButton:
	    text: "Up"
	    md_bg_color: 0, 0, 0.8, 0.5
	    pos_hint: {"center_x": 0.35, "center_y": 0.5}
	    on_press: app.vertical_control_positive(*args)

	MDRaisedButton:
	    text: "Down"
	    md_bg_color: 0, 0, 0.8, 0.5
	    pos_hint: {"center_x": 0.45, "center_y": 0.5}
	    on_press: app.vertical_control_negative(*args)

	MDLabel:
		id: Veritcal_Position
		text: "Value"
	    pos_hint: {"center_x": 1.4, "center_y": 0.5}
		size_hint: 1,1

	MDLabel:
		id: Spacer
		text: "Set Value: "
	    pos_hint: {"center_x": 1.2, "center_y": 0.5}
		size_hint: 1,1











	MDLabel:
		id: Tile_angle
		text: "Pour Angle Limit:"
		font_size: '22sp'
	    pos_hint: {"center_x": 0.55, "center_y": 0.7}
		size_hint: 1,1
	
	MDLabel:
		id: Tile_angle_value_text
		text: "Value"
	    pos_hint: {"center_x": 1.4, "center_y": 0.7}
		size_hint: 1,1

	MDLabel:
		id: Spacer
		text: "Set Value: "
	    pos_hint: {"center_x": 1.2, "center_y": 0.7}
		size_hint: 1,1


	MDRaisedButton:
	    text: "90"
		# font_size: '24sp'
	    md_bg_color: 0, 0, 1, 0.5
	    pos_hint: {"center_x": 0.35, "center_y": 0.7}
	    # on_press: app.tilt_angle_control_positive(*args)
		on_press: app.set_tilt_angle([0, 90])

	MDRaisedButton:
	    text: "120"
		# font_size: '24sp'
	    md_bg_color: 0, 0, 1, 0.5
	    pos_hint: {"center_x": 0.44, "center_y": 0.7}
	    on_press: app.set_tilt_angle([-0.55, 120])

	MDRaisedButton:
	    text: "150"
		# font_size: '24sp'
	    md_bg_color: 0, 0, 1, 0.5
	    pos_hint: {"center_x": 0.53, "center_y": 0.7}
	    # on_press: app.tilt_angle_control_positive(*args)
	    on_press: app.set_tilt_angle([-1.1, 150])

	MDRaisedButton:
	    text: "180"
		# font_size: '24sp'
	    md_bg_color: 0, 0, 1, 0.5
	    pos_hint: {"center_x": 0.62, "center_y": 0.7}
	    # on_press: app.tilt_angle_control_negative(*args)
	    on_press: app.set_tilt_angle([-1.65, 180])



	MDLabel:
		id: Max_Velocity 
		text: "Rotational Velocity:"
		font_size: '22sp'
	    pos_hint: {"center_x": 0.55, "center_y": 0.8}
		size_hint: 1,1
	
	MDLabel:
		id: Max_Velocity_value_text
		text: "Value"
	    pos_hint: {"center_x": 1.4, "center_y": 0.8}
		size_hint: 1,1
	
	MDLabel:
		id: Spacer
		text: "Current Value: "
	    pos_hint: {"center_x": 1.2, "center_y": 0.8}
		size_hint: 1,1

	MDRaisedButton:
	    text: "Slow"
		# font_size: '24sp'
	    md_bg_color: 0, 0, 1, 0.5
	    pos_hint: {"center_x": 0.35, "center_y": 0.8}
	    on_press: app.max_vel_control_positive(*args)

	MDRaisedButton:
	    text: "Medium"
		# font_size: '24sp'
	    md_bg_color: 0, 0, 1, 0.5
	    pos_hint: {"center_x": 0.45, "center_y": 0.8}
	    on_press: app.max_vel_control_negative(*args)

	MDRaisedButton:
	    text: "Fast"
		# font_size: '24sp'
	    md_bg_color: 0, 0, 1, 0.5
	    pos_hint: {"center_x": 0.55, "center_y": 0.8}
	    # on_press: app.max_vel_control_negative(*args)





	MDRaisedButton:
	    text: "Execute Pour"
		font_size: '24sp'
	    md_bg_color: 0, 0, 1, 0.5
		size_hint: (0.2, 0.1)
	    pos_hint: {"center_x": 0.5, "center_y": 0.2}
	    on_press: app.execute_pour_action(*args)

	MDRaisedButton:
	    text: "Reset"
		# font_size: '24sp'
	    md_bg_color: 0, 0, 1, 0.5
		# size_hint: (0.2, 0.1)
	    pos_hint: {"center_x": 0.95, "center_y": 0.95}
	    on_press: app.execute_reset_pour_position(*args)

